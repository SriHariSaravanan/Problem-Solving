import java.util.*;
public class Main{
    public static void main(String[] args){
        int[] nums={1,2,3,4,1};
        Arrays.sort(nums);
        int n=nums.length;
        for(int i=0;i<=n-1;i++){
            if(nums[i]==nums[i+1]){
                return true;
            }
        }
        return false;
    }
}
